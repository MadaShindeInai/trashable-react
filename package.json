{
  "name": "trashable-react",
  "version": "1.0.3",
  "description": "A HOC to make React Components garbage collectable when unmounted.",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "lint": "eslint src/** __tests__/**",
    "test": "jest",
    "validate": "npm run lint && npm test",
    "prepush": "npm run validate",
    "prepublish": "npm run validate && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hjylewis/trashable-react.git"
  },
  "keywords": [
    "react",
    "promise",
    "javascript",
    "cancelable",
    "garbage-collect",
    "trashable"
  ],
  "author": "hjylewis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hjylewis/trashable-react/issues"
  },
  "homepage": "https://github.com/hjylewis/trashable-react#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint": "^4.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-react": "^7.4.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "react": "^16.1.0",
    "react-dom": "^16.1.0"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0"
  },
